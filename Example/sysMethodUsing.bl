(print "Sprawdzanie obecnosci plikow .cpp w katalogu (Windows)...\n")
(def test (input "podaj format:"))
(def command ("dir /b . | findstr " + test))
(def result (sys command))
(if ((len result) > 0) (do (print "Sukces! Znaleziono nastepujace pliki " test ":\n") (print result)))
